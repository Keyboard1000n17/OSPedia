#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/clean-css-cli@5.6.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/cleancss" "$@"
else
  exec node  "$basedir/../.pnpm/clean-css-cli@5.6.3/node_modules/clean-css-cli/bin/cleancss" "$@"
fi
