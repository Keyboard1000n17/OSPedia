#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules/@netlify/zip-it-and-ship-it/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules/@netlify/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules/@netlify/zip-it-and-ship-it/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules/@netlify/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.3.0_supports-color@10.2.2/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
