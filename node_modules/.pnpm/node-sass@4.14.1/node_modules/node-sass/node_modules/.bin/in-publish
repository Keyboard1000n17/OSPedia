#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/in-publish@2.0.1/node_modules/in-publish/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/in-publish@2.0.1/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/in-publish@2.0.1/node_modules/in-publish/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/in-publish@2.0.1/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../in-publish@2.0.1/node_modules/in-publish/in-publish.js" "$@"
else
  exec node  "$basedir/../../../../../in-publish@2.0.1/node_modules/in-publish/in-publish.js" "$@"
fi
