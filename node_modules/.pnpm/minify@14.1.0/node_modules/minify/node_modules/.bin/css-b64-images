#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules/css-b64-images/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules/css-b64-images/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules/css-b64-images/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules/css-b64-images/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/css-b64-images@0.2.5/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../css-b64-images@0.2.5/node_modules/css-b64-images/bin/css-b64-images" "$@"
else
  exec node  "$basedir/../../../../../css-b64-images@0.2.5/node_modules/css-b64-images/bin/css-b64-images" "$@"
fi
