#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/build/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/build/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/build/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@netlify+build@35.5.10_@opentelemetry+api@1.8.0_@types+node@25.0.10_picomatch@4.0.3/node_modules/@netlify/build/bin.js" "$@"
fi
