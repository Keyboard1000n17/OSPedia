#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules/netlify-cli/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules/netlify-cli/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules/netlify-cli/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules/netlify-cli/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/netlify-cli@23.13.5_@types+node@25.0.10_picomatch@4.0.3/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run.js" "$@"
else
  exec node  "$basedir/../../bin/run.js" "$@"
fi
