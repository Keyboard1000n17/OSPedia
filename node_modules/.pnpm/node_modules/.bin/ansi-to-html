#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules/ansi-to-html/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules/ansi-to-html/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules/ansi-to-html/bin/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules/ansi-to-html/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/ansi-to-html@0.7.2/node_modules:/home/keyboard1000n17/Documents/OSPedia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ansi-to-html/bin/ansi-to-html" "$@"
else
  exec node  "$basedir/../ansi-to-html/bin/ansi-to-html" "$@"
fi
